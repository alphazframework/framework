{
    "name": "zest/framework",
    "description": "The Zest Framework.",
    "keywords": ["framework", "zest"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "authors": [
        {
            "name": "Malik Umer Farooq",
            "email":"lablnet01@gmail.com",
            "homepage": "https://softhub99.com"
        }
    ],
    "autoload": {
        "psr-4": {
             "Zest\\": "src/"
        },
        "files":["src/functions/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Framework\\Tests\\": "Tests/"
        }
    },
    "config":{
        "optimize-autoloader":true
    },
    "prefer-stable": true
}
